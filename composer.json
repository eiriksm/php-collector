{
    "name": "zaporylie/php-collector",
    "authors": [
        {
            "name": "Jakub Piasecki",
            "email": "jakub@piaseccy.pl"
        }
    ],
    "scripts": {
        "test": "phpunit ./test/unit/",
        "test-style": "phpcs --standard=PSR2 -p src",
        "fix-style": "phpcbf --standard=PSR2 -p src"
    },
    "require": {
        "php": "^5.4 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Collector\\": "src/Collector/",
            "Collector\\Request\\": "src/Collector/Request/",
            "Collector\\Response\\": "src/Collector/Response/",
            "Collector\\Data\\": "src/Collector/Data/"
        }
    }
}
